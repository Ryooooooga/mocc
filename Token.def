#ifndef TOKEN
#define TOKEN(name, text)
#endif

#ifndef TOKEN_KW
#define TOKEN_KW(name, text) TOKEN(name, text)
#endif

#ifndef TOKEN_KW1
#define TOKEN_KW1(w) TOKEN_KW(kw_##w, #w)
#endif

#ifndef TOKEN_DELIM
#define TOKEN_DELIM(name, text) TOKEN(name, text)
#endif

TOKEN(identifier, "identifier")
TOKEN(number, "number")
TOKEN(string, "string")

TOKEN_KW1(typedef)
TOKEN_KW1(const)
TOKEN_KW1(void)
TOKEN_KW1(char)
TOKEN_KW1(int)
TOKEN_KW1(struct)
TOKEN_KW1(sizeof)
TOKEN_KW1(if)
TOKEN_KW1(else)
TOKEN_KW1(while)
TOKEN_KW1(return)

TOKEN_DELIM(arrow, "->")

#undef TOKEN
#undef TOKEN_KW
#undef TOKEN_KW1
#undef TOKEN_DELIM
